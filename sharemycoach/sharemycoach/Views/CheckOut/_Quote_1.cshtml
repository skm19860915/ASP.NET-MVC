<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-validator/0.4.5/js/bootstrapvalidator.min.js"></script>
<script type="text/javascript">
    var vehicle_key, daily_rate, life_date, interval, cleaning_fee, prep_fee, deposit, hours, miles, dateDeparture, dateReturn, minimumNumberOfTimeInterval, calcByNights;
    var additional_equipment = 0, stickyWidth, screenWidth, screenHeight;
    $(function(){
        vehicle_key = @Html.Raw(Json.Encode(info.QuickFindKeyWord));
        daily_rate = getDecimalValue(@info.HigherRate);
        cleaning_fee = getDecimalValue(@info.WebCleaningFee);
        prep_fee = getDecimalValue(@info.WebPrepFee);
        deposit = getDecimalValue(@info.WebRefundableSecurityDeposit);
        hours = getDecimalValue(@info.WebGeneratorFreeHours);
        miles = getDecimalValue(@info.WebIncludesTheseMilesFreePerDay);
        minimumNumberOfTimeInterval = getDecimalValue(@info.MinimumNumberOfTimeInterval);
        calcByNights = GetBooleanValue(@Json.Encode(info.CalcByNights));

        getInitDates();
        loadStickyForm();

        var e = document.getElementById('element');
        var s = document.getElementById('sticky-tab');

        $(window).resize(function(){
            screenWidth = $("#mobile-search-bar").width();
            var hiddenTabPosition = screenWidth - stickyWidth + 210;
            var shownTabPosition = screenWidth - stickyWidth - 63;
            var shownStickyPosition = screenWidth - stickyWidth - 27;
            if(e.style.display == 'none'){
                s.style.marginLeft = hiddenTabPosition + 'px';
                $('#element').hide();
            }
            else{
                s.style.marginLeft = shownTabPosition + 'px';
                e.style.marginLeft = shownStickyPosition + 'px';
                $('#element').show();
            }
        })

        function loadStickyForm()
        {
            var interval_val, interval_day;
            var departure_date = getFormatDate(dateDeparture);
            var return_date = getFormatDate(dateReturn);
            life_date = departure_date + ' - ' + return_date;
            interval = getDateInterval(dateDeparture, dateReturn);
            if(calcByNights > 0)
            {
                interval_val = interval;
                interval_day = interval_val + ' nights';
            }
            else
            {
                interval_val = interval + 1;
                interval_day = interval_val + ' days';
            }

            window.onload = document.getElementById('element').innerHTML =
            '<div style="border: 2px solid darkgrey"><div style="background-color:black; padding-bottom:45px; padding-left:15px; padding-right:15px; padding-top:20px;">' +
            '<span style="padding-right:40px; float:left; color:white; font-size:18px; font-family:Lato; font-weight:bold;">Veh #' + vehicle_key +
            '</span><span style="float:right; color:white; font-size:18px; font-family:Lato; font-weight:bold;">$' + daily_rate + ' per day</span></div>' +
            '<div class="estimateDiv"><span style="color:#555; font-family:Lato; font-weight:bold;">' + life_date + '</span></div>' +
            '<span style="padding-left:15px; float:left; color:#555; font-family:Lato; font-weight:bold;">$' + daily_rate + '&nbsp;&#215;&nbsp;' + interval_day +
            '</span><span style="padding-right:15px; float:right; color:#555; font-family:Lato; font-weight:bold;">$' + Math.floor(daily_rate * interval_val) + '</span><br />' +
            '<span style="padding-left:15px; float:left; color:#555; font-family:Lato; font-weight:bold;">Cleaning</span>' +
            '<span style="padding-right:15px; float:right; color:#555; font-family:Lato; font-weight:bold;">$' + cleaning_fee + '</span><br />' +
            '<span style="padding-left:15px; float:left; font-family:Lato; font-weight:bold;">Prep.&nbsp;&nbsp;Fee</span>' +
            '<span style="padding-right:15px; float:right; font-family:Lato; font-weight:bold;">$' + prep_fee + '</span><br />' +
            '<span style="padding-left:15px; float:left; font-family:Lato; font-weight:bold;">Add.&nbsp;&nbsp;&nbsp;Equipment</span>' +
            '<span style="padding-right:15px; float:right; font-family:Lato; font-weight:bold;">$' + additional_equipment + '</span>' +
            '<br /><hr style="margin:10px; border:0; border-top:1px solid black;" />' +
            '<span style="padding-left:15px; float:left; color:#555; font-family:Lato; font-weight:bold;">Total</span>' +
            '<span style="padding-right:15px; float:right; color:#555; font-family:Lato; font-weight:bold;">$' +
            Math.floor(daily_rate * interval_val + cleaning_fee + prep_fee + additional_equipment) + '</span>' +
            '<br /><hr style="margin-top:10px; margin-bottom:10px; border:0; border-top:1px solid #e1e1e1;" />' +
            '<span style="padding-left:15px; float:left; color:#555; font-family:Lato; font-weight:bold;">Security Deposit</span>' +
            '<span style="padding-right:15px; float:right; color:#555; font-family:Lato; font-weight:bold;">$' + deposit + '</span><br />' +
            '<div style="margin-top:10px;"><span style="padding-left:15px;color:black; font-size:14px; font-family:Lato;">Includes <b>' +
            Math.floor(interval_val * hours) + '</b> Free Hours Generator</span></div>' +
            '<h5 style="padding-left:15px;color:black; font-weight:normal;">Includes <b>' + Math.floor(interval_val * miles) + '</b> Free Miles</h5><br /><br />';

            screenHeight = $(window).height();
            screenWidth = $("#mobile-search-bar").width();
            var stickyHeight = $("#element").outerHeight();
            stickyWidth = $("#element").width();

            $("#element").css('top', screenHeight - stickyHeight +'px');
            $("#element").css('position', 'fixed');
            $("#sticky-tab").css('top', screenHeight - stickyHeight +'px');
            $("#sticky-tab").css('position', 'fixed');
            var shownTabPosition = screenWidth - stickyWidth - 63;
            var shownStickyPosition = screenWidth - stickyWidth - 27;
            $('#sticky-tab').css('margin-left', shownTabPosition + 'px');
            $('#element').css('margin-left', shownStickyPosition + 'px');
            //$('#element').css('display', 'block');
        }

        function getDecimalValue(value)
        {
            if(value == null)
                return 0;
            else
                return value;
        }

        function GetBooleanValue(value)
        {
            if(value == null || value == false)
                return 0;
            else
                return 1;
        }

        function getFormatDate(date)
        {
            var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
            var format = new Date(date);
            var day = format.getDate();
            var monthIndex = format.getMonth();
            var year = format.getFullYear();

            return monthNames[monthIndex] + ' ' + day + ', ' + year;
        }

        function getDateInterval(date1, date2)
        {
            var departure_date = new Date(date1);
            var return_date = new Date(date2);
            var interval = Math.floor((return_date - departure_date)/(24*60*60*1000));

            return interval;
        }

        function getInitDates()
        {
            if(sessionStorage.getItem("dateDeparture") === undefined || sessionStorage.getItem("dateDeparture") == null || sessionStorage.getItem("dateDeparture") == "undefined"){
                var today = new Date();
                dateDeparture = today.setDate(today.getDate() + 2);
            }
            else{
                dateDeparture = sessionStorage.getItem("dateDeparture");
            }

            $('#departureDate').datepicker({
                format: 'mm/dd/yyyy',
                startDate: new Date(dateDeparture),
            });
            $('#departureDate').datepicker('setDate', new Date(dateDeparture));

            if(sessionStorage.getItem("dateReturn") === undefined || sessionStorage.getItem("dateReturn") == null || sessionStorage.getItem("dateReturn") == "undefined"){
                dateReturn = today.setDate(today.getDate() + minimumNumberOfTimeInterval - calcByNights);
            }
            else{
                dateReturn = sessionStorage.getItem("dateReturn");
            }

            $('#returnDate').datepicker({
                format: 'mm/dd/yyyy',
                startDate: new Date(dateReturn),
            });
            $('#returnDate').datepicker('setDate', new Date(dateReturn));
        }

        $("#sticky-tab").click(function(e){
            e.preventDefault();
            var element = document.getElementById('element');
            if(element.style.display == 'none')
            {
                var shownTabPosition = screenWidth - stickyWidth - 63;
                var shownStickyPosition = screenWidth - stickyWidth - 27;
                $('#sticky-tab').css('margin-left', shownTabPosition + 'px');
                $('#element').css('margin-left', shownStickyPosition + 'px');
                $('#element').show();
            }
            else
            {
                var hiddenTabPosition = screenWidth - stickyWidth + 210;
                $('#sticky-tab').css('margin-left', hiddenTabPosition + 'px');
                $('#element').hide();
            }
        });

        $('#departureDate').datepicker().on('changeDate', function (e) {
            var d_change_date = e.date;
            var date_obj = new Date(d_change_date);
            var r_change_date = date_obj.setDate(date_obj.getDate() + minimumNumberOfTimeInterval - calcByNights);
            $('#returnDate').datepicker('setStartDate', new Date(r_change_date));
            $('#returnDate').datepicker('setDate', new Date(r_change_date));
            dateDeparture = d_change_date;
            dateReturn = r_change_date;
            loadStickyForm();
        });

        $('#returnDate').datepicker().on('changeDate', function(e){
            dateReturn = e.date;
            loadStickyForm();
        });

        $('#additional_equipment :checkbox').change(function(){
            if ($(this).is(':checked')) {
                var add_price = 0;
                var add_value = $(this).parent().parent().find('.web_price').text();
                if(add_value == "" || add_value == null){
                    add_price = 0;
                }
                else{
                    add_price = parseInt(add_value.split("$").pop());
                }
                additional_equipment += add_price;
            }
            else if($(this).is(':checked') == false){
                var del_price = 0;
                var del_value = $(this).parent().parent().find('.web_price').text();
                if(del_value == "" || del_value == null){
                    del_price = 0;
                }
                else{
                    del_price = parseInt(del_value.split("$").pop());
                }
                additional_equipment -= del_price;
            }
            loadStickyForm();
        });
    });
</script>
<script>
    $('#equipment_handler a').click(function (e) {
        $('#additional_equipment').toggle();
        var status = $('#additional_equipment').css('display');
        if(status == 'none'){
            document.getElementById('equipment_link').innerHTML =
                '<h5 style="padding:12px; margin-bottom:0px; text-align:center; background-color:#4B4B4B;">Click here to Show Optional Items</h5>';
        }
        if(status == 'block'){
            document.getElementById('equipment_link').innerHTML =
                '<h5 style="padding:12px; margin-bottom:0px; text-align:center; background-color:#4B4B4B;">Click here to Hide Optional Items</h5>';
        }
    });
    $('#equipment_handler').hover(function () {
        $('#equipment_handler a h5').css("color", "yellow");
    });
    $('#equipment_handler').mouseleave(function () {
        $('#equipment_handler a h5').css("color", "white");
    });
</script>
<script>
    $(document).on('click', '#form_submit', function(){
        var validator = $('#QuoteForm').data('bootstrapValidator');
        var city_value = document.getElementById('city').value;
        var state_value = document.getElementById('state').value;
        var zip_value = document.getElementById('zip').value;
        if(city_value){
            validator.enableFieldValidators('city', false);
        }
        if(state_value){
            validator.enableFieldValidators('state', false);
        }
        if(zip_value){
            validator.enableFieldValidators('zip', false);
        }
    });
</script>