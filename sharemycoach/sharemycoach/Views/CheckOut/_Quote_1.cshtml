<script>
    var placeSearch, autocomplete, geocoder;

    function getRadianValue(x)
    {
        return x * Math.PI /180;
    }

    function getDistance(latt1, long1, latt2, long2)
    {
        var R = 6378137;
        var dLat = getRadianValue(latt2 - latt1);
        var dLong = getRadianValue(long2 - long1);
        var value1 = Math.sin(dLat / 2) * Math.sin(dLat / 2) + Math.cos(getRadianValue(latt1)) * Math.cos(getRadianValue(latt2)) * Math.sin(dLong / 2) * Math.sin(dLong / 2);
        var value2 = 2 * Math.atan2(Math.sqrt(value1), Math.sqrt(1 - value1));
        var result1 = R * value2; // distance as meter
        var result = result1 * 0.000621371192; // distance as mile
        return result;
    }

    function initAutocomplete()
    {
        geocoder = new google.maps.Geocoder();
        destination = new google.maps.places.Autocomplete(
          (document.getElementById('destination')), {
              types: ['geocode']
          });

        destination.addListener('place_changed', fillInAddress);

        address = new google.maps.places.Autocomplete(
            (document.getElementById('address')), {
                types: ['geocode']
            });
        address.addListener('place_changed', fillInAddress1);
    }

    function fillInAddress()
    {
        var place = destination.getPlace();
        codeAddress(document.getElementById('destination').value);
    }

    function codeAddress(address)
    {
        geocoder.geocode({
            'address': address
        }, function (results, status) {
            if (status == 'OK') {
                var val = getDistance(results[0].geometry.location.lat(), results[0].geometry.location.lng(), @(info.Latitude ?? 0), @(info.Longitude ?? 0));
                document.getElementById('trip').value = Math.floor(val * 2);
                //$('#trip').focus();
                if($('#trip-div').hasClass('has-error')){
                    $('#trip-div').removeClass('has-error');
                    $('#trip-div small:eq(1)').css('display', 'none');
                }
            } else {
                alert('Geocode was not successful for the following reason: ' + status);
            }
        });
    }

    function fillInAddress1()
    {
        var place = address.getPlace();
        codeAddress1(document.getElementById('address').value);
    }

    function codeAddress1(address)
    {
        geocoder.geocode({'address': address
        }, function (results, status) {
            if (status == 'OK')
            {
                var array = results[0].address_components;
                if(array.length < 6){
                    alert('Invalid Address!! Please try again!');
                    return;
                }
                if(array.length == 6){
                    document.getElementById('real_address').value = array[0].long_name + ' ' + array[1].long_name;
                    document.getElementById('city').value = array[1].long_name;
                    document.getElementById('state').value = array[3].short_name;
                    document.getElementById('zip').value = array[5].long_name;
                }
                if(array.length == 7){
                    document.getElementById('real_address').value = array[0].long_name + ' ' + array[1].long_name;
                    document.getElementById('city').value = array[2].long_name;
                    document.getElementById('state').value = array[4].short_name;
                    document.getElementById('zip').value = array[6].long_name;
                }
                if(array.length == 8){
                    document.getElementById('real_address').value = array[0].long_name + ' ' + array[1].long_name;
                    document.getElementById('city').value = array[2].long_name;
                    document.getElementById('zip').value = array[7].long_name;
                    if(array[5].short_name == "US"){
                        document.getElementById('state').value = array[4].short_name;
                    }
                    else{
                        document.getElementById('state').value = array[5].short_name;
                    }
                }
                if(array.length == 9){
                    document.getElementById('real_address').value = array[0].long_name + ' ' + array[1].long_name;
                    document.getElementById('city').value = array[3].long_name;
                    document.getElementById('state').value = array[5].short_name;
                    document.getElementById('zip').value = array[7].long_name;
                }

                if($('#city-div').hasClass('has-error')){
                    $('#city-div').removeClass('has-error');
                    $('#city-div small:eq(1)').css('display', 'none');
                }
                if($('#state-div').hasClass('has-error')){
                    $('#state-div').removeClass('has-error');
                    $('#state-div small:eq(1)').css('display', 'none');
                }
                if($('#zip-div').hasClass('has-error')){
                    $('#zip-div').removeClass('has-error');
                    $('#zip-div small:eq(1)').css('display', 'none');
                    $('#zip-div small:eq(2)').css('display', 'none');
                }
            }
            else
            {
                alert('Geocode was not successful for the following reason: ' + status);
            }
        });
    }
</script>
